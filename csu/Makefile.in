#
# top-level Makefile
#

TARGOS = @targos@
TARGMACH = @targmach@
TARGET = @target@
VERSION = @version@

MFLAGS += MACH=$(TARGMACH) TARGET=$(TARGET) VERSION=$(VERSION)

all clean install:
	if test -d $(TARGOS) ; then cd $(TARGOS) && $(MAKE) $(MFLAGS) $@; fi

distclean: clean
